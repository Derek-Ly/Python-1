    "#KMeans with elbow to find optimal n_cluster\n",
    "elbow = []\n",
    "k_elbow = list(range(1, 10))\n",
    "\n",
    "for i in k_elbow:\n",
    "    km = KMeans(i, random_state= 30)\n",
    "    km.fit(standard_data)\n",
    "    elbow.append(km.inertia_)\n",
    "    \n",
    "#Elbow Plot, optimal n_cluster = 3\n",
    "plt.plot(k_elbow, elbow, '-o')"
   ]
